{
	"compilerOptions": {
		"allowJs": true,
		"composite": false,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"inlineSources": false,
		"isolatedModules": true,
		"lib": [
			"ESNext"
		],
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"newLine": "lf",
		"noEmit": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"preserveWatchOutput": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ESNext"
	},
	"exclude": [
		"**/node_modules"
	]
}
